{
  "name": "flosum_agent",
  "version": "0.0.28-B1",
  "description": "Flosum Agent",
  "main": "index.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "start": "node .",
    "docs": "aglio -i docs/index.apib --server --port 3030",
    "dev": "nodemon server/index.ts",
    "test": "jest",
    "compile": "ts-node build.ts && tsc --build tsconfig.build.json && babel dist -d dist",
    "build-version": "npm install && npm run compile && bash build.version.sh"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint server --fix",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "debug": "^4.3.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.2",
    "express-openapi-validator": "^4.12.11",
    "helmet": "^4.4.1",
    "jszip": "^3.6.0",
    "nocache": "^2.1.0",
    "reflect-metadata": "^0.1.13",
    "sfdx-cli": "7.109.0",
    "shelljs": "^0.8.4",
    "typedi": "^0.10.0",
    "uuid": "^8.3.2",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.12",
    "@types/express-http-proxy": "^1.6.1",
    "@types/faker": "^5.5.7",
    "@types/jest": "^26.0.23",
    "@types/jszip": "^3.4.1",
    "@types/node": "^15.6.1",
    "@types/shelljs": "^0.8.8",
    "@types/uuid": "^8.3.0",
    "@types/xml2js": "^0.4.9",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "faker": "^5.5.3",
    "javascript-obfuscator": "^2.15.5",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.1",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.4.2"
  }
}
